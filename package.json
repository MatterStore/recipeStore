{
  "type": "module",
  "name": "recipestore-backend",
  "version": "1.0.0",
  "description": "Backend for MERN application",
  "main": "backend/dist/app.js",
  "scripts": {
    "start": "node backend/dist/app.js",
    "test": "tsc && NODE_ENV=testing mocha 'backend/dist/tests/**/*.js' --recursive --exit --timeout 10000",
    "dev": "npm run postinstall && npm start & cd frontend && npm start",
    "preinstall": "cd frontend && npm install",
    "postinstall": "tsc",
    "heroku-postbuild": "cd frontend && npm install && npm run build",
    "prepare": "husky install",
    "pretty-quick": "pretty-quick"
  },
  "author": "MatterStore",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "joi": "^17.6.0",
    "js-to-ts-converter": "^0.18.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^6.5.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "replace-require-with-import": "^2.1.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/chai": "^4.3.3",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.1.1",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^17.0.29",
    "@types/supertest": "^2.0.12",
    "chai": "^4.3.6",
    "husky": "^8.0.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.19",
    "prettier": "2.7.1",
    "pretty-quick": "^3.1.3",
    "supertest": "^6.2.4",
    "typescript": "^4.7.4"
  }
}
